<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.twtims.mapboxdemo">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".MapApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".label.BasicSymbolLayerActivity"></activity>
        <activity android:name=".label.MarkerFollowingRouteActivity" />
        <activity android:name=".displaymap.IndoorMapActivity" />
        <activity android:name=".label.ClickAddPhotoActivity" />
        <activity android:name=".displaymap.ZoomDependentFillColorActivity" />
        <activity android:name=".label.LanguageSwitchActivity" />
        <activity android:name=".layer.ColorSwitcherActivity" />
        <activity android:name=".label.SpaceStationLocationActivity" />
        <activity android:name=".label.AnimatedMarkerActivity" />
        <activity android:name=".layer.AdjustLayerOpacityActivity" />
        <activity android:name=".layer.ImageSourceTimeLapseActivity" />
        <activity android:name=".layer.AddWmsSourceActivity" />
        <activity android:name=".layer.VectorLayerActivity" />
        <activity android:name=".layer.GeojsonLayerInStackActivity" />
        <activity android:name=".layer.HillShadeActivity" />
        <activity android:name=".displaymap.FragmentMapActivity" />
        <activity android:name=".displaymap.DynamicMapActivity" />
        <activity android:name=".NavActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".displaymap.MainActivity" />
    </application>

</manifest>